generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Level {
  id    Int     @id @default(autoincrement())
  name  String  @unique
  users User[]
}

model User {
  id        Int      @id @default(autoincrement())
  fullName  String
  email     String   @unique
  password  String
  levelId   Int
  level     Level    @relation(fields: [levelId], references: [id])
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
